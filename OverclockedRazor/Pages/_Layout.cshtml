<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OverclockedRazor</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css" />
    <link href="~/css/site.css" rel="stylesheet" />
    <!--    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />-->
</head>
<body class="bg-dark text-light">
   <nav class="navbar navbar-expand-md navbar-dark" style="background-color: black;">
      <a class="navbar-brand" asp-page="/Index" style="font-family: Russo One;">OVERCLOCKED FITNESS</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" asp-page="/Candidate">Start-Up</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" asp-page="/About">About</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" asp-page="/Contact">Contact</a>
          </li>
        </ul>
      </div>
    </nav>
    <div class="body-content">
        @RenderBody()
        <hr />
    </div>
    <footer class="footer text-center py-4 bg-primary">
            <div class="col m-auto">
                <a href="http://facebook.com/OverclockedFitness" alt="Facebook Page"><img class="img-fluid" src="./img/social/facebook.png" width="60" alt="Facebook"></a>
                <a href="http://twitter.com/OverclockedFitness" alt="Twitter"><img class="img-fluid" src="./img/social/twitter.png" width="60" alt="Twitter"></a>
                <a href="http://www.tumblr.com/OverclockedFitness" alt="Tumblr... yes, we do that too."><img class="img-fluid" src="./img/social/tumblr.png" width="60" alt="Tumblr"></a>
            </div>
        <p class="small">&copy; 2017 - Overclocked Fitness</p>
    </footer>
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/js/bootstrap.min.js" integrity="sha384-a5N7Y/aK3qNeh15eJKGWxsqtnX/wWdSZSKp+81YjTmS15nvnvxKHuzaWwXHDli+4" crossorigin="anonymous"></script>
<!--        <script src="~/js/site.min.js" asp-append-version="true"></script>-->

    @RenderSection("Scripts", required: false)
</body>
</html>
